{
  "name": "markdown",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "react-scripts": "0.7.0",
    "shelljs": "^0.7.8",
    "surge": "^0.19.0"
  },
  "dependencies": {
    "marked": "0.3.6",
    "react": "15.3.2",
    "react-dom": "15.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
